{
    "manufacturer": "ssianen",
    "keyboard_name": "maxipad88",
    "maintainer": "ssianen",
    "bootloader": "rp2040",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "encoder": true,
        "oled": true,
        "mousekey": true,
        "rgblight": true,
        "nkro": true
    },

    "processor": "RP2040",
    "url": "",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },

    "rgblight": {
        "animations": {
            "breathing": true
        },
        "driver": "ws2812",
        "default": {
            "animation": "breathing",
            "val": 255,
            "sat": 255,
            "hue": 64
        }
    },

    "ws2812": {
        "pin": "GP16",
        "driver": "vendor"
    },

    "encoder": {
        "rotary": [
            {"pin_a": "GP26", "pin_b": "GP28"}
        ]
    },

    "matrix_pins": {
        "direct":[  [null, null, "GP22", null],
                    ["GP2", "GP3", "GP4", "GP5"],
                    ["GP6", "GP7", "GP8", "GP9"],
                    ["GP10", "GP11", "GP12", "GP13"],
                    ["GP14", "GP15", "GP18", "GP17"]
                ]
    },

    "layouts": {
        "LAYOUT": {
            "layout": [

                { "matrix": [0, 2], "x": 0, "y": 0 },

                { "matrix": [1, 0], "x": 1, "y": 0 },
                { "matrix": [1, 1], "x": 1, "y": 0 },
                { "matrix": [1, 2], "x": 2, "y": 0 },
                { "matrix": [1, 3], "x": 3, "y": 0 },

                { "matrix": [2, 0], "x": 0, "y": 1 },
                { "matrix": [2, 1], "x": 1, "y": 1 },
                { "matrix": [2, 2], "x": 2, "y": 1 },
                { "matrix": [2, 3], "x": 3, "y": 1 },

                { "matrix": [3, 0], "x": 0, "y": 2 },
                { "matrix": [3, 1], "x": 1, "y": 2 },
                { "matrix": [3, 2], "x": 2, "y": 2 },
                { "matrix": [3, 3], "x": 3, "y": 2 },

                { "matrix": [4, 0], "x": 0, "y": 3 },
                { "matrix": [4, 1], "x": 1, "y": 3 },
                { "matrix": [4, 2], "x": 2, "y": 3 },
                { "matrix": [4, 3], "x": 3, "y": 3 }
            ]
        }
    }


}
